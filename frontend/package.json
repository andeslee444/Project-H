{
  "name": "mental-health-system-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "ANALYZE=true tsc && vite build",
    "build:cdn": "USE_CDN=true tsc && vite build",
    "build:sourcemap": "SOURCE_MAP=true tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "bundle:check": "npm run build && ls -lah dist/js/*.js | awk '{print $5, $9}'",
    "clean": "rm -rf dist node_modules/.vite",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:docs": "storybook build --docs",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:codegen": "playwright codegen http://localhost:5173",
    "test:e2e:report": "playwright show-report",
    "test:e2e:trace": "playwright show-trace",
    "test:e2e:chrome": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:auth": "playwright test tests/e2e/specs/auth/",
    "test:e2e:appointments": "playwright test tests/e2e/specs/appointments/",
    "test:e2e:providers": "playwright test tests/e2e/specs/providers/",
    "test:e2e:waitlist": "playwright test tests/e2e/specs/waitlist/",
    "test:e2e:notifications": "playwright test tests/e2e/specs/notifications/"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-switch": "^1.2.5",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-query-devtools": "^5.80.6",
    "@types/dompurify": "^3.0.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.513.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.14.0",
    "react-toastify": "^11.0.5",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "usehooks-ts": "^3.1.1",
    "zod": "^3.25.51",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.28.0",
    "@playwright/test": "^1.52.0",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.9.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "storybook": "^8.6.14",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vitest": "^3.2.2"
  }
}
